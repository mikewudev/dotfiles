{
    "coc.preferences.formatOnSaveFiletypes": ["php", "vue", "javascript", "javascriptreact", "typescript", "typescriptreact"],
    "eslint.enable": true,
    "eslint.filetypes": ["javascript", "javascriptreact", "typescript", "typescriptreact", "vue"],
    "snippets.priority": 100,
    "typescript.suggest.completeFunctionCalls": false,
    "javascript.suggest.completeFunctionCalls": false,
    "typescript.preferences.importModuleSpecifier": "non-relative",
    "javascript.preferences.importModuleSpecifier": "non-relative",
    "intelephense.licenceKey":  "004dcqcilf01mhe",
    "prettier.arrowParens": "always",
    "prettier.bracketSpacing": true,
    "prettier.printWidth": 80,
    "prettier.proseWrap": "always",
    "prettier.semi": false,
    "prettier.singleQuote": true,
    "prettier.tabWidth": 2,
    "prettier.useTabs": false,
    "prettier.trailingComma": "all",
    "prettier.jsxBracketSameLine": false,
    "prettier.jsxSingleQuote": false,
    "prettier.disableLanguages": [],
    "vetur.experimental.templateInterpolationService": true,
    "vetur.validation.template": false
}

